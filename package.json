{
  "name": "@sozdev/bd",
  "type": "module",
  "version": "1.0.0",
  "packageManager": "yarn@3.6.0",
  "description": "The official Blank Cloud Sandbox Template by the CodeSandbox team",
  "author": "Alexander Semyenov <semyenov@hotmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/codesandbox-app/static-template#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codesandbox-app/static-template.git"
  },
  "bugs": {
    "url": "https://github.com/codesandbox-app/static-template/issues"
  },
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "index.d.ts",
  "files": [
    "dist",
    "*.d.ts"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "nr build --watch",
    "lint": "eslint ./src/**/*.{ts,vue,js}",
    "lint:fix": "eslint --fix ./src/**/*.{ts,vue,js}",
    "prepublishOnly": "npm run build",
    "release": "bumpp --commit --push --tag && npm publish",
    "start": "esno src/index.ts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "consola": "^3.2.2",
    "natural": "^6.5.0",
    "readline": "^1.3.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.39.6",
    "@rollup/plugin-alias": "^5.0.0",
    "@rollup/plugin-commonjs": "^25.0.2",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@types/natural": "^5.1.2",
    "@types/node": "^20.3.2",
    "@yarnpkg/doctor": "^4.0.0-rc.46",
    "@yarnpkg/sdks": "^3.0.0-rc.46",
    "bumpp": "^9.1.1",
    "eslint": "^8.43.0",
    "esno": "^0.16.3",
    "nr": "^0.1.0",
    "rollup": "^3.25.3",
    "rollup-plugin-dts": "^5.3.0",
    "rollup-plugin-esbuild": "^5.0.0",
    "serve": "^11.2.0",
    "tslib": "^2.6.0",
    "typescript": "^5.1.5"
  }
}
